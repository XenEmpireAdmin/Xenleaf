<?xml version="1.0" encoding="utf-8" ?>
<Plants>
  <!--=========================== Crops ==============================-->

  
  <ThingDef ParentName="PlantBase">
    <defName>PlantXenleaf</defName>
    <label>Xenleaf plant</label>
    <description>A leafy plant cultivated for its psychological effects. Xenleaf can be prepared into joints at the crafting spot.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Beauty>3</Beauty>
    </statBases>
    <graphicData>
      <texPath>Plants/XenLeaf</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>10</pathCost>
    <ingestible>
      <nutrition>0.1</nutrition>
    </ingestible>
    <plant>
			<fertilitySensitivity>1.0</fertilitySensitivity>
			<growDays>6.84</growDays>
			<dieIfLeafless>true</dieIfLeafless>
			<reproduces>false</reproduces>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>XenleafLeaves</harvestedThingDef>
			<harvestDestroys>true</harvestDestroys>
			<harvestYield>11</harvestYield>
			<sowMinSkill>4</sowMinSkill>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<topWindExposure>0.1</topWindExposure>
			<visualSizeRange>
        <min>0.3</min>
        <max>1.0</max>
      </visualSizeRange>
    </plant>
  </ThingDef>



</Plants>